<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="15dp"
    android:orientation="vertical"
    tools:context=".MainActivity">
    <LinearLayout
        android:id="@+id/LinearLayout_ont_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
    <Spinner
        android:id="@+id/spin_one_1"
        android:layout_weight="2"
        android:layout_width="25dp"
        android:layout_height="32dp"
        android:entries="@array/data3"
        android:prompt="@string/include_name"
        android:spinnerMode="dialog" />
    <Spinner
        android:id="@+id/spin_one_2"
        android:layout_weight="3"
        android:layout_width="40dp"
        android:layout_height="32dp"
        android:entries="@array/data1"
        android:prompt="@string/keyword_name"
        android:spinnerMode="dialog" />
    <EditText
        android:id="@+id/text_1"
        android:layout_weight="2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="16sp"
        android:inputType="text" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayout_two_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
        <Spinner
            android:id="@+id/spin_two_1"
            android:layout_weight="2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:entries="@array/data3"
            android:prompt="@string/include_name"
            android:spinnerMode="dialog" />
        <Spinner
            android:id="@+id/spin_two_2"
            android:layout_weight="3"
            android:layout_width="40dp"
            android:layout_height="25dp"
            android:entries="@array/data1"
            android:prompt="@string/keyword_name"
            android:spinnerMode="dialog" />
        <EditText
            android:id="@+id/text_2"
            android:layout_weight="2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="16sp"
            android:inputType="text" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayout_three_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
        <Spinner
            android:id="@+id/spin_three_1"
            android:layout_weight="2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:entries="@array/data3"
            android:prompt="@string/include_name"
            android:spinnerMode="dialog" />
        <Spinner
            android:id="@+id/spin_three_2"
            android:layout_weight="3"
            android:layout_width="40dp"
            android:layout_height="25dp"
            android:entries="@array/data1"
            android:prompt="@string/keyword_name"
            android:spinnerMode="dialog" />
        <EditText
            android:id="@+id/text_3"
            android:layout_weight="2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="16sp"
            android:inputType="text" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayout_four_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
        <Spinner
            android:id="@+id/spin_four_1"
            android:layout_weight="2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:entries="@array/data3"
            android:prompt="@string/include_name"
            android:spinnerMode="dialog" />
        <Spinner
            android:id="@+id/spin_four_2"
            android:layout_weight="3"
            android:layout_width="40dp"
            android:layout_height="25dp"
            android:entries="@array/data1"
            android:prompt="@string/keyword_name"
            android:spinnerMode="dialog" />
        <EditText
            android:id="@+id/text_4"
            android:layout_weight="2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="16sp"
            android:inputType="text" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayout_five_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >
        <Spinner
            android:id="@+id/spin_five_1"
            android:layout_weight="2"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:entries="@array/data3"
            android:prompt="@string/include_name"
            android:spinnerMode="dialog" />
        <Spinner
            android:id="@+id/spin_five_2"
            android:layout_weight="3"
            android:layout_width="40dp"
            android:layout_height="25dp"
            android:entries="@array/data1"
            android:prompt="@string/keyword_name"
            android:spinnerMode="dialog" />
        <EditText
            android:id="@+id/text_5"
            android:layout_weight="2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="16sp"
            android:inputType="text" />
    </LinearLayout>
    <Button
        android:id="@+id/highButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_gravity="center"
        android:text="搜索"
        android:minHeight="0dp"
        android:minWidth="0dp"
        android:textColor="#fff"
        android:textSize="16sp"/>
    <View
        android:layout_width="fill_parent"
        android:layout_marginTop="5dp"
        android:layout_height="2dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:background="#aa000000" />

    <ListView
        android:id="@+id/search_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1" >
    </ListView>
</LinearLayout>